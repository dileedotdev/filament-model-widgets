{
    "name": "dinhdjj/filament-model-widgets",
    "description": "Package for quickly generating widgets of models",
    "keywords": [
        "dinhdjj",
        "laravel",
        "filament-model-widgets"
    ],
    "homepage": "https://github.com/dinhdjj/filament-model-widgets",
    "license": "MIT",
    "authors": [
        {
            "name": "dinhdjj",
            "email": "dinhdjj@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^2.15",
        "flowframe/laravel-trend": "^0.1.1",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Dinhdjj\\FilamentModelWidgets\\": "src",
            "Dinhdjj\\FilamentModelWidgets\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dinhdjj\\FilamentModelWidgets\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dinhdjj\\FilamentModelWidgets\\FilamentModelWidgetsServiceProvider"
            ],
            "aliases": {
                "FilamentModelWidgets": "Dinhdjj\\FilamentModelWidgets\\Facades\\FilamentModelWidgets"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
